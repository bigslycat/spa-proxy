{
  "name": "spabox",
  "version": "2.0.5",
  "description": "Docker image as DevOps tool for delivery single-page applications",
  "repository": "bigslycat/spabox",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "prebuild": "mkdir -p lib",
    "build": "rollup -c && nexe build/build-nginx-config.js -t alpine-x64-12.4.0 -o bin/build-nginx-config",
    "postbuild": "chmod +x bin/* && rm -rf buildd",
    "build:docker": "./push",
    "build:docker-test": "docker build -t spabox-test:latest .",
    "preversion": "git checkout master",
    "postversion": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "core-js": "^3.1.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "nexe": "^3.3.2",
    "nginxbeautify": "^2.0.1",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "typed-contracts": "^2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "flow-bin": "^0.102.0",
    "flow-typed": "^2.5.2",
    "husky": "^3.0.0",
    "jlens": "^1.0.4",
    "prettier": "^1.18.2"
  }
}
