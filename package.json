{
  "name": "spabox",
  "version": "2.0.4",
  "description": "Docker image as DevOps tool for delivery single-page applications",
  "repository": "bigslycat/spabox",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "build": "rollup -c && chmod +x bin/*",
    "build:docker": "./push",
    "build:docker-test": "docker build -t spabox-test:latest .",
    "precommit": "yarn lint && yarn flow",
    "preversion": "git checkout master && yarn precommit",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "core-js": "^2.5.7",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "nginxbeautify": "^2.0.1",
    "rollup": "^0.65.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "typed-contracts": "^1.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.5.1",
    "husky": "^0.14.3",
    "jlens": "^1.0.4",
    "prettier": "^1.14.2"
  }
}
