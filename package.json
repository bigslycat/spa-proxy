{
  "name": "spabox",
  "version": "2.0.4",
  "description": "Docker image as DevOps tool for delivery single-page applications",
  "repository": "bigslycat/spabox",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "build": "rollup -c && chmod +x bin/*",
    "build:docker": "./push",
    "build:docker-test": "docker build -t spabox-test:latest .",
    "preversion": "git checkout master && yarn precommit",
    "postversion": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-flow": "^7.0.0",
    "core-js": "^3.0.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "nginxbeautify": "^2.0.1",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "typed-contracts": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.97.0",
    "flow-typed": "^2.5.1",
    "husky": "^1.3.1",
    "jlens": "^1.0.4",
    "prettier": "^1.17.0"
  }
}
